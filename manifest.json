{
  "manifest_version": 2,
  "name": "Symfony Docs Version Selector",
  "description": "Select your Symfony documentation version. Will redirect all requests to correct version.",
  "version": "0.0.2",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Symfony Docs Version Selector",
    "default_popup": "options.html"
  },
  "permissions": [
      "storage",
      "webRequest",
      "webRequestBlocking",
      "*://symfony.com/*"
    ]
}
